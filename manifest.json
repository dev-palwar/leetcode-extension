{
    "manifest_version": 3,
    "name": "LeetCode Submission Fetcher",
    "version": "1.0",
    "permissions": [
        "cookies",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://leetcode.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}