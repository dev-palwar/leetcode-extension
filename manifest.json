{
    "manifest_version": 2,
    "name": "LeetCode Solved Problems",
    "version": "1.0",
    "description": "Fetches your solved LeetCode problems",
    "permissions": [
        "https://leetcode.com/*",
        "storage",
        "<all_urls>"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'; connect-src *",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {}
    },
    "icons": {}
}